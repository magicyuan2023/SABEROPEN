@using Juyou.Solution.Model.Models;
@using Juyou.Solution.Model;
@{ Layout = null; }
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>@SystemSet.SystemName</title>
    <meta name="renderer" content="webkit">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <link rel="stylesheet" href="~/layui/css/layui.css">
    <link rel="stylesheet" href="~/css/main.css">
    <script type="text/javascript" src="~/layui/layui.js"></script>
    <script type="text/javascript" src="~/js/jquery.js"></script>
    <script type="application/javascript" src="~/js/site.js"></script>
   
    @RenderSection("Styles", required: false)
</head>
<body leftmargin=0 topmargin=0 oncontextmenu='return false' ondragstart='return false'>
    @* onselectstart='return false' onselect='emptySelectSection()' oncopy='emptySelectSection()' onbeforecopy='return false' onmouseup='emptySelectSection()'> *@
    @RenderBody()
    <script>

        function emptySelectSection() {
            if (document.selection != null) {
                document.selection.empty();
            }
        }
    </script>
    @RenderSection("Scripts", required: false)
</body>
</html>
